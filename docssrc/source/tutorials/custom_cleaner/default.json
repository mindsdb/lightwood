{
    "features": {
        "excerpt": {
            "encoder": {
                "module": "PretrainedLangEncoder",
                "args": {
                    "output_type": "$dtype_dict[$target]",
                    "stop_after": "$problem_definition.seconds_per_encoder"
                }
            },
            "data_dtype": "rich_text"
        }
    },
    "outputs": {
        "target": {
            "data_dtype": "float",
            "encoder": {
                "module": "NumericEncoder",
